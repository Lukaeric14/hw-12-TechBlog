{{#each posts as |post|}}
  <div class="row mb-4 project">
    <div class="col-md-5">
      <h3>
        <a href="/post/{{post.id}}">{{post.title}}</a>
      </h3>
      <span>Created by
        {{post.user.username}}
        on
        {{post.date_created}}</span>
    </div>
    <div class="col-md-7">
      <p>
        {{post.contents}}
      </p>
    </div>
  </div>
{{/each}}